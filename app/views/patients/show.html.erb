<h1><%= @patient.name %> Profile Page</h1>

<strong><p>Age:</strong> <%= @patient.age %></p>
<strong><p>Appointments:</strong></p>
<% @patient.appointments.each do |appt| %>
  <ul>
    <li><%= appt.format_appt%></li>
    <li><%= link_to appt.doctor.name, doctor_path(appt.doctor) %></li>
  </ul>
<% end %>
